{
  "frameio": {
    "account_id": null,
    "workspace_id": null,
    "project_id": null,
    "base_url": "https://api.frame.io/v4",
    "max_concurrent_uploads": 3,
    "oauth": {
      "client_id": null,
      "client_secret": null,
      "jwt_key": null,
      "token_url": "https://auth.frame.io/oauth2/token"
    }
  },
  "discord": {
    "webhook_url": null,
    "bot_name": "RL PostFlow Bot",
    "enable_notifications": true,
    "notification_types": {
      "upload_success": true,
      "upload_error": true,
      "batch_summary": true,
      "system_status": false
    }
  },
  "lucidlink": {
    "by_shot_path": "/path/to/lucidlink/BY_SHOT",
    "supported_extensions": [".mov", ".mp4", ".avi", ".mkv", ".prores", ".mxf", ".r3d", ".braw"],
    "min_file_size": 1048576,
    "processing_delay": 10,
    "move_processed_files": false,
    "move_failed_files": true,
    "file_stability_check": true,
    "stability_wait_time": 5,
    "scan_interval": 5
  },
  "processing": {
    "max_concurrent_uploads": 3,
    "chunk_size": 8388608,
    "retry_attempts": 3,
    "retry_delay": 30,
    "timeout": 300,
    "queue_max_size": 100
  },
  "logging": {
    "level": "INFO",
    "file": "logs/frameio_integration.log",
    "max_size": 10485760,
    "backup_count": 5,
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "enable_console": true,
    "enable_file": true
  },
  "monitoring": {
    "stats_interval": 60,
    "health_check_interval": 300,
    "enable_metrics": true,
    "metrics_file": "logs/metrics.json"
  },
  "cache": {
    "structure_file": "config/frameio_structure.json",
    "processed_files_cache": "data/processed_files.json",
    "enable_cache": true,
    "cache_ttl": 3600
  },
  "integration_settings": {
    "auto_start": true,
    "graceful_shutdown_timeout": 30,
    "restart_on_error": true,
    "max_restart_attempts": 3
  }
}
